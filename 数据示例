0. {126567.691349, 68842.179691, 0}	        {111914.609383, 69957.264618, 0}
            {0;1}	                          {155204.522111, 65313.273794, 0}
0. {111914.609383, 69957.264618, 0}	        {164406.622283, 108172.245513, 0}
            {0;2}                          	{116144.805638, 108504.05393, 0}
0. {155204.522111, 65313.273794, 0}	        {110409.996766, 107313.262337, 0}
            {0;3}	                          {121698.355897, 69339.231214, 0}
0. {164406.622283, 108172.245513, 0}	      {124684.266462, 77265.180056, 0}
